<script language="javascript">

function jsFunction(){
var odiv=document.getElementById('Loadstyle');
 $("#Loadstyle").mouseover(function(){
var leftx=  $("#Loadstyle").offset().left+"px";
var topy= $("#Loadstyle").offset().top+20+"px";
var pp="absolute; z-index: 99999; top:"+ topy+"; left:"+ leftx+"; display:black;";
    $("#ms_rt_gl_msglLoadDetail").attr("style",pp);
     });
 $("#Loadstyle").mouseleave(function(){
 $("#ms_rt_gl_msglLoadDetail").attr("style"," display:none;");
       	 });

}
   
//var weekDay = new Array('Sun','Mon','Tue','Wed','Thu','Fri','Sat');
function getWeekDay(){
	var tradeDate = new Date("04/04/2022");
	if(tradeDate==null || typeof(tradeDate)=="undifined"){
		 $("#asOfDate").html("-")+" "+"data not available";
	}else{
		//$("#asOfDate").html( weekDay[tradeDate.getDay()]+" "+formatDateByCul(tradeDate));
		$("#asOfDate").html(formatDateByCul(tradeDate));
	}
	
	var tradeDate2 = new Date("04/04/2022");
	if(tradeDate2==null || typeof(tradeDate2)=="undifined"){
		 $("#oneDayReturnAsOfDate").html("-")+" "+"data not available";
	}else{
		//$("#oneDayReturnAsOfDate").html( weekDay[tradeDate2.getDay()]+" "+formatDateByCul(tradeDate2));
		$("#oneDayReturnAsOfDate").html(formatDateByCul(tradeDate2));
	}
}
function formatDateByCul (date){
var culture="en-US";
	if(culture==null || typeof(culture)=="undifined"){
				return this.formatDate(date,"MM/DD/yyyy");
			}
			if(culture.toLowerCase().replace("-","_").indexOf("fr_ca") >=0){
				return this.formatDate(date,"DD-MM-yyyy");
			}else if(culture.toLowerCase().replace("-","_").indexOf("fr_fr") >=0){
				return this.formatDate(date,"DD/MM/yyyy");
			}else if(culture.toLowerCase().replace("-","_").indexOf("en_ca") >=0){
				return this.formatDate(date,"DD/MM/yyyy");
			}else{
				return this.formatDate(date,"MM/DD/yyyy");
			}
	
	
}
function formatDate(date,formatter){
	var hour = date.getHours();
	var minute = date.getMinutes();
	var year = date.getUTCFullYear();
	var month = date.getMonth()+1;
	var day = date.getDate();
	day=(day<10)?"0"+day:day;
	month=(month<10)?"0"+month:month;
	var timeValue = "" +((hour >= 12) ? "PM" : "AM");
	hour = ((hour >12) ? hour -12 :hour);
	minute = (minute<10) ? "0"+minute:minute;
	return formatter.replace("MM",month).replace("yyyy",year).replace("DD",day);
}
function getCurSymbol(){
	var currency="USD";
			switch(currency.toUpperCase()){
			case "USD":
				return "$";
			case "GBX":
				return "￡";
			case "GBP":
				return "￡";
			case "EUR":
				return "€";
			case "THB":
			   return "฿";
			case "CHF":
			   return "SFr.";
			case "KRW":
			   return "₩";
			default: return "&nbsp;";
			}
		}



var curSymbol=getCurSymbol();
$("#cursymbol").html(curSymbol);
$("#cursymboloftotal").html(curSymbol);
//getWeekDay();
   </script>
    
