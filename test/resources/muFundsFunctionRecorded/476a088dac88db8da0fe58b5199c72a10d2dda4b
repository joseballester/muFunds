<script>
    if (!true)
        $(".stock__content-articles").addClass("hidebutton");

    $("#relateArticles").scrollLeft(0);
    $("#btnCaretLeft").addClass("hidebutton");

    function next() {
        const width = $("#relateArticlesP").width();
        scrollBy(width)
    }

    function previous() {
        const width = $("#relateArticlesP").width();
        scrollBy(-width)
    }

    function scrollBy(offset) {
        const inner = $("#relateArticles");
        const newPosition = inner.scrollLeft() + offset;
        console.log("newPosition" + newPosition);
        inner.scrollLeft(newPosition);
        if (newPosition <= 0)
            $("#btnCaretLeft").addClass("hidebutton");
        else 
            $("#btnCaretLeft").removeClass("hidebutton");
        if (newPosition >= document.getElementById('relateArticles').scrollWidth - offset)
            $("#btnCaretRight").addClass("hidebutton");
        else
            $("#btnCaretRight").removeClass("hidebutton");
    }
</script>
