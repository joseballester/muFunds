    <script type="text/javascript">
        $(document).ready(function () {
            //Load righ column contetn module
            getContentModule("268,269");
        });

        function getContentModule(lineupIdIn) {
            const lineupId = lineupIdIn.split(',');
            $.ajax({
                url: "../Services/CMSgoLineupHandler.ashx",
                data: { "lineupId": aiMatchCore.ShowUserType().indexOf('a') > -1 ? parseInt(lineupId[0]) : parseInt(lineupId[1]) },
                success: function (response) {
                    $("#contentModuleDiv").html(response);
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }
    </script>

