        var adFrame1 = document.getElementById("TopAd");
        var adFrame1txt = adFrame1.innerHTML;
        if ((adFrame1txt.indexOf("aimatch.com/default.gif") >= 0) || (adFrame1txt.indexOf("aimatch.com/empty.gif") >= 0) || (adFrame1txt.indexOf("morningstar.com/empty.gif") >= 0) || (adFrame1txt.trim().length < 200)) {
